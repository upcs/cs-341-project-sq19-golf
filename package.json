{
  "name": "up-scheduler",
  "version": "1.0.0",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && sudo PORT=80 node ./src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest -u --coverage && codecov > /dev/null"
  },
  "author": "Team Golf",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/mocks/**",
      "!*.config.js",
      "!**/index.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./tests/setup.tests.js",
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "ag-grid-community": "^20.2.0",
    "ag-grid-react": "^20.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-regenerator-runtime": "^6.5.0",
    "body-parser": "^1.18.3",
    "browser-filesaver": "^1.1.1",
    "codecov": "^3.3.0",
    "cors": "^2.8.5",
    "dedupe-plugin": "^1.0.1",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "horizontal-scroll": "^1.1.0",
    "html2canvas": "^1.0.0-rc.1",
    "iconv-lite": "^0.4.24",
    "identity-obj-proxy": "^3.0.0",
    "jest-pytest": "^0.7.0",
    "jquery": "^3.4.0",
    "jquery-suggest": "^1.1.6",
    "js-autocomplete": "^1.0.4",
    "js-combinatorics": "^0.5.4",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "net": "^1.0.2",
    "node-fetch": "^2.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fade-in": "^0.1.6",
    "react-inline-predict": "^2.0.1",
    "react-native": "^0.59.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-native": "^4.3.0",
    "react-table": "^6.9.2",
    "reactjs-popup": "^1.3.2",
    "redux": "^4.0.1",
    "request": "^2.88.0",
    "sequelize": "^4.43.1",
    "tedious": "^5.0.3",
    "tls": "0.0.1",
    "uglify-js-plugin": "0.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
